{
  "name": "vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --mode=development",
    "prod": "vite --host --mode=production",
    "build:production": "tsc --noEmit && vite build --mode=production",
    "build:development": "tsc && vite build --mode=development",
    "preview": "vite preview",
    "dev:farm": "farm start --mode=development",
    "prod:farm": "farm start --mode=production",
    "build:dev:farm": "tsc --noEmit && farm build --mode=development",
    "build:farm": "tsc --noEmit && farm build --mode=development",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "dependencies": {
    "@icon-park/react": "^1.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.45.1",
    "@tonconnect/ui-react": "^2.0.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@twa-dev/sdk": "^6.7.1",
    "@types/node": "^20.14.7",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "ahooks": "^3.8.0",
    "antd": "^5.18.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.3",
    "clsx": "^2.1.1",
    "eruda": "^3.0.1",
    "js-audio-recorder": "^1.0.7",
    "lodash": "^4.17.21",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.2",
    "react-router-dom": "^6.23.1",
    "sass": "^1.77.6",
    "simple-zustand-devtools": "^1.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "vite-boilerplate": "file:",
    "vite-tsconfig-paths": "^4.3.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "daisyui": "^4.12.7",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
